---
import type { MarkdownLayoutProps } from 'astro';
import DocLayout from './DocLayout.astro';
import type { SEOFrontmatter } from '../types/seo';

type Props = MarkdownLayoutProps<SEOFrontmatter & {
  title: string;
  description?: string;
}>;

const { frontmatter } = Astro.props;
---

<DocLayout title={frontmatter.title} description={frontmatter.description} frontmatter={frontmatter}>
  <slot />
</DocLayout>
